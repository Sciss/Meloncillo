; MAX/MSP REALTIME PLUG-IN FOR USE WITH THE IRCAM SPAT~ EXTERNALS(setq realtime T); returns NIL if an error occurs(defun my-bundle-send (timetag bundle)	(not (null (osc-bundle-send-and-wait 'scosc timetag bundle "/synced" 3000))))(load (path-concat (gethash "BASEDIRECTORY" prefs) "mx-spat.lisp"))(defun cleanup NIL (progn	(if (oboundp 'scsync) (progn		(osc-bundle-send 'scsync 0.0 (list (list "/notify" 0)))		(datagram-channel-close 'scsync)	))	(if (oboundp 'sctraj) (byte-buffer-free 'sctraj) NIL)	(if (oboundp 'scosc) (progn		(osc-bundle-send 'scosc 0.0 (list (list "/tschuessikowsky")))		(datagram-channel-close 'scosc)	))		T	; success))